{
  "name": "@stefanprobst/next-svg",
  "version": "4.2.0-canary.2",
  "main": "src/index.js",
  "type": "module",
  "repository": "git@github.com:stefanprobst/next-svg.git",
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./src/index.d.ts",
      "import": "./src/index.js"
    },
    "./svg-symbol-loader": "./src/svg-symbol-loader.cjs",
    "./svg-inline-loader": "./src/svg-inline-loader.cjs",
    "./types": "./src/svg.d.ts"
  },
  "files": [
    "src"
  ],
  "scripts": {
    "format": "prettier . -l --ignore-path .gitignore",
    "format:fix": "yarn format --write",
    "lint": "eslint . --cache --ignore-path .gitignore",
    "lint:fix": "yarn lint --fix",
    "prepare": "simple-git-hooks || true",
    "validate": "yarn format && yarn lint"
  },
  "dependencies": {
    "hast-util-from-parse5": "^6.0.1",
    "hast-util-to-estree": "^1.4.0",
    "parse5": "^6.0.1",
    "recast": "^0.20.5",
    "svgo-loader": "^3.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^15.0.0",
    "@stefanprobst/commitlint-config": "^1.0.2",
    "@stefanprobst/prettier-config": "^2.0.1",
    "@types/svgo": "^2.6.0",
    "@types/webpack": "^5.28.0",
    "eslint": "^8.3.0",
    "lint-staged": "^12.1.2",
    "next": "^12.0.4",
    "prettier": "^2.5.0",
    "simple-git-hooks": "^2.7.0"
  },
  "peerDependencies": {
    "next": ">=12.0.0"
  },
  "engines": {
    "node": ">=14.17",
    "yarn": "1.x"
  },
  "commitlint": {
    "extends": [
      "@stefanprobst/commitlint-config"
    ]
  },
  "eslintConfig": {
    "root": true,
    "reportUnusedDisableDirectives": true,
    "extends": [
      "eslint:recommended"
    ],
    "parserOptions": {
      "sourceType": "module",
      "ecmaVersion": 2020
    },
    "env": {
      "node": true
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --cache --fix",
      "prettier --write"
    ],
    "*.@(json|md)": "prettier --write"
  },
  "prettier": "@stefanprobst/prettier-config",
  "simple-git-hooks": {
    "commit-msg": "yarn commitlint --edit",
    "pre-commit": "yarn lint-staged",
    "pre-push": "yarn run validate"
  }
}
