{
  "name": "@stefanprobst/next-svg",
  "version": "1.0.0",
  "main": "src/index.js",
  "types": "src/index.d.ts",
  "repository": "git@github.com:stefanprobst/next-svg.git",
  "author": "Stefan Probst <stefanprobst@kolabnow.com>",
  "license": "MIT",
  "files": [
    "src"
  ],
  "scripts": {
    "format": "prettier . -l --ignore-path .gitignore",
    "format:fix": "yarn format --write",
    "lint": "eslint . --ignore-path .gitignore",
    "lint:fix": "yarn lint --fix"
  },
  "dependencies": {
    "@stefanprobst/svgo-loader": "^1.0.0",
    "@svgr/plugin-jsx": "^5.4.0",
    "@svgr/webpack": "^5.4.0",
    "url-loader": "^4.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@stefanprobst/commitlint-config": "^1.0.0",
    "@stefanprobst/eslint-config-node": "^1.0.2",
    "@stefanprobst/prettier-config": "^1.0.0",
    "@typescript-eslint/eslint-plugin": "^4.2.0",
    "@typescript-eslint/parser": "^4.2.0",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^24.0.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.3.0",
    "jest": "^26.4.2",
    "lint-staged": "^10.4.0",
    "next": "^9.5.3",
    "prettier": "^2.1.2",
    "react": "^16.13.1",
    "typescript": "^4.0.3"
  },
  "peerDependencies": {
    "next": "^9.5.0",
    "react": "^16.10.0"
  },
  "engines": {
    "node": ">=12.9",
    "yarn": "^1"
  },
  "commitlint": {
    "extends": [
      "@stefanprobst/commitlint-config"
    ]
  },
  "eslintConfig": {
    "extends": [
      "@stefanprobst/eslint-config-node"
    ],
    "overrides": [
      {
        "files": [
          "*.ts"
        ],
        "parser": "@typescript-eslint/parser",
        "extends": "plugin:@typescript-eslint/recommended",
        "rules": {
          "node/no-unsupported-features/es-syntax": "off"
        }
      }
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.(js|ts)": "eslint --cache --fix",
    "*.(json|md)": "prettier --write"
  },
  "prettier": "@stefanprobst/prettier-config"
}
